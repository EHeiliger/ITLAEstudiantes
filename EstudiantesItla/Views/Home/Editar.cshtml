
@model  EstudiantesItla.Models.ListaModeloTabla



<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>AddNuevo</title>
    <style>
        .campo {
            margin-top: 20px;
        }
    </style>
</head>
<body>
    <div>
        
        <form action="/home/GuardarCambio/" method="post">

            <label for="nombre">Nombre</label>
            <input  type="text" id="nombre" name="Nombre" value="@Model.Nombre" /> <br />
            <label for="Apellido">Apellido</label>
            <input type="text" id="Apellido" class="campo" name="Apellido" value="@Model.Apellido" /> <br />
            <label for="Carrera">Carrera</label>
            <input type="text" id="Carrera" class="campo" name="Carrera" value="@Model.Carrera" /> <br />
            <label for="Status">Status</label>
            <input type="text" id="Status" class="campo" name="Status" value="@Model.Status" /> <br />
            <label for="Matricula">Matricula</label>
            <input readonly="readonly" type="text" id="Matricula" class="campo" name="Matricula" value="@Model.Matricula" /> <br />
            <button type="submit" style="margin-top: 10px">Editar</button>
        </form>
        
    </div>
</body>
</html>